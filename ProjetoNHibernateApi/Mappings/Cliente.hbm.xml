<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2">
	<class name="ProjetoNHibernateApi.Models.Domain.Entities.Cliente, ProjetoNHibernateApi" table="cliente">
		<id name ="Id" column="id">
			<generator class="identity" />
		</id>
		<property name ="Nome" column="nome"/>
		<property name ="Email" column="email" />
		<property name ="Fone" column="fone" />
		<property name ="DataNascimento" column="datanascimento" />
		<property name ="Divida" column="divida" />
		

	</class>
</hibernate-mapping>
